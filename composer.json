{
    "name": "shengyou/teamcity-hooks",
    "description": "a teamcity repo for webhook demo",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Shengyou Fan",
            "email": "shengyoufan@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "^5.7",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "squizlabs/php_codesniffer": "^3.4",
        "phpro/grumphp": "^0.15.0"
    },
    "scripts": {
        "check-style": [
            "phpcs --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
        ],
        "fix-style": [
            "phpcbf --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
        ]
    }
}
